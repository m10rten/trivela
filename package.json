{
  "name": "trivela",
  "version": "0.1.0",
  "type": "module",
  "description": "Creative approach to building full scale applications.",
  "scripts": {
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint ./**/*.ts --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "reset": "pnpm i && pnpm -r build",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "m10rten",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.22.0",
    "@trivela/config": "workspace:*",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "eslint": "9.22.0",
    "husky": "9.1.7",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vitest": "3.0.8"
  },
  "engines": {
    "node": ">=22 <24",
    "pnpm": ">=9.0 <9.20"
  }
}
